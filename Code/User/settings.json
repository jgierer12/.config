{
  "editor.fontFamily": "'Dank Mono', monospace",
  "editor.fontSize": 20,
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "'Dank Mono', monospace",
  "terminal.integrated.fontSize": 18,
  "workbench.colorTheme": "Atom One Light",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#528BFF80"
  },
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 1,
  "window.menuBarVisibility": "toggle",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "window.restoreWindows": "none",
  "workbench.panel.defaultLocation": "right",
  "editor.rulers": [80],
  "breadcrumbs.enabled": true,
  "workbench.editor.showTabs": false,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "boundary",
  "extensions.autoUpdate": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.autoIndent": true,
  "editor.dragAndDrop": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "git.autofetch": true,
  "git.enableCommitSigning": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "html.format.enable": false,
  "json.format.enable": true,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.lineNumbers": "relative",
  "eslint.autoFixOnSave": true,
  "eslint.options": {
    "ignorePath": ".gitignore"
  },
  "prettier.ignorePath": ".gitignore",
  "vim.visualstar": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },
    {
      "before": ["<leader>", "w", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "q", "q"],
      "commands": ["workbench.action.closeWindow"]
    },
    {
      "before": ["<leader>", "w", "q"],
      "commands": [
        "workbench.action.files.save",
        "workbench.action.closeWindow"
      ]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<C-w>"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":noh"]
    },
    {
      "before": ["<leader>", "\""],
      "commands": [":reg"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["editor.action.moveLinesDownAction"]
    }
  ],
  "vim.handleKeys": {
    "<C-u>": false,
    "<C-b>": false,
    "<C-d>": false
  }
}
